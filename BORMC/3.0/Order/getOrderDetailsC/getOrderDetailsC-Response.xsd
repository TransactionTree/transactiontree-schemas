			<xs:documentation>Monetary/quantity decimal that also accepts empty string.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:decimal EmptyString"/>
	</xs:simpleType>
	<xs:simpleType name="ResponseCode">
		<xs:annotation>
			<xs:documentation>Response codes from the service.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="E101">
				<xs:annotation>
					<xs:documentation>XML Input is empty</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E102">
				<xs:annotation>
					<xs:documentation>XML SAX Exception</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E103">
				<xs:annotation>
					<xs:documentation>XML Parse Conf Exception</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E104">
				<xs:annotation>
					<xs:documentation>XML IOException</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E111">
				<xs:annotation>
					<xs:documentation>Person Not Found</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E122">
				<xs:annotation>
					<xs:documentation>From Date / Thru Date Parse Exception</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E124">
				<xs:annotation>
					<xs:documentation>Order Status exceeds 30 Characters</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E126">
				<xs:annotation>
					<xs:documentation>No order found</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E127">
				<xs:annotation>
					<xs:documentation>Invalid or Store tag is blank</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E129">
				<xs:annotation>
					<xs:documentation>No order found</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E899">
				<xs:annotation>
					<xs:documentation>accessTokenKey is not valid</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TaxType">
