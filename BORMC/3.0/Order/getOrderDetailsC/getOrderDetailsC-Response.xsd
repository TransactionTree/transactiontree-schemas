					<xs:documentation>Tax number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TAX_VALUE_EXEMPT" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Exempt value.</xs:documentation>
				</xs:annotation>
