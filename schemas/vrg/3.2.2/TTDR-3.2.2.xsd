				<xs:element name="TransactionAssociate" minOccurs="1">
					<xs:annotation>
						<xs:documentation>The TransactionAssociate element provides information about the associate or clerk involved in the transaction. This element is required and includes the following sub-elements:</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ClerkIDType" minOccurs="1">
								<xs:annotation>
									<xs:documentation>Specifies the type of ID used to identify the clerk or associate. Possible values include OperatorID, EmployeeID, SalespersonID, and UserID.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:NMTOKEN">
										<xs:enumeration value="OperatorID"/>
										<xs:enumeration value="EmployeeID"/>
										<xs:enumeration value="SalespersonID"/>
										<xs:enumeration value="UserID"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<!-- Clerk Id assigned to transaction -->
							<xs:element name="ClerkId" type="xs:string" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>The value associated with the ClerkIDType. This identifier must be between 1 and 9 alphanumeric characters.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<!-- Workday Id assigned to transaction added 10/16/24 -->
							<xs:element name="WorkdayId" type="xs:string" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Clerk WOrkdayID or other ID. Added 10/16/24</xs:documentation>
								</xs:annotation>
							</xs:element>
							<!-- Clerk Name assigned to transaction -->
							<xs:element name="ClerkName" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The name of the person associated with the ClerkId. This name must be between 1 and 30 characters in length.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="30"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ClerkRole" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Specifies the role of the clerk or associate in the transaction. Possible values include Sale, Quote, Delivery, Other, and Manager.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:NMTOKEN">
										<xs:enumeration value="Sale"/>
										<xs:enumeration value="Quote"/>
										<xs:enumeration value="Delivery"/>
										<xs:enumeration value="Other"/>
										<xs:enumeration value="Manager"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
