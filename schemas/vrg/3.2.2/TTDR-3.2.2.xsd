															<xs:documentation>The name of the cardholder.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="FsaCard" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Indicates if the card used is an FSA (Flexible Spending Account) card.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ReaderEntryMode" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The entry mode used by the reader for the card.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CvResponse" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The card verification response.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ErrorMessage" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The error message, if any, from the transaction.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ExtraData" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Any extra data related to the transaction.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Rfmiq" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Reserved field for future use.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DebitTraceNumber" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The trace number for debit transactions.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CardBin" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The Bank Identification Number (BIN) of the card used for the payment.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="FinalTaxLines" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Represents the final tax lines for the entire transaction or order. This element captures multiple tax details that are applied at the transaction level. This element is not specific to line items but to the overall transaction.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<xs:element name="TaxAmount" type="xs:integer" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The monetary amount of the tax line.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxPercent" type="xs:decimal" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The percentage rate of the tax line.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxDescription" type="xs:string" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Description of the tax line.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxAmountExempt" type="xs:integer" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Amount of the tax line that is exempt</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/amountExempt.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxDetailNumber" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Line number</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/detailNumber.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxDetailNumberVoided" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Line number voided</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/detailNumberVoided.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxNumber" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The tax number within the transaction</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/taxNumber.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxRuleCode" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The tax rule</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/taxRuleCode.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxValueExempt" type="xs:integer" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Value that is exempt from this rule.</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/valueExempt.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxAmountTaxable" type="xs:integer" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The amount this tax rule is applied to.</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/valueTaxable.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxLineVoided" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>If true then this line has been voided.</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/voided.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="TaxLineVoidingLine" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Parent number of voiding line.</xs:documentation>
												<xs:appinfo>
													<application name="FlooidPOS">
														<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnTaxes/voidingLine.</xs:documentation>
													</application>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="TotalsTransactionData" minOccurs="1">
								<xs:complexType>
									<xs:all>
										<xs:element name="TotalTax" minOccurs="0">
											<xs:annotation>
												<xs:documentation>All the tax lines added up. Used is only one final tax line.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="TotalTaxAmount" type="xs:integer" minOccurs="0"/>
													<xs:element name="TaxPercent" type="xs:decimal" minOccurs="0"/>
													<xs:element name="TotalTaxDescription" type="xs:string" minOccurs="0"/>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="Discounts" minOccurs="0">
											<xs:complexType>
												<xs:all>
													<xs:element name="AdditionalDiscountDescription" type="xs:string" minOccurs="0"/>
													<xs:element name="AdditionalDiscountAmount" type="xs:integer" minOccurs="0"/>
													<xs:element name="AdditionalDiscountPercent" type="xs:integer" minOccurs="0"/>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="TransactionLevelDiscounts" minOccurs="0">
											<xs:annotation>
												<xs:documentation>
      Added on 2025-06-02 to handle discounts applied at the transaction/order/invoice level, which cannot be associated directly with specific line items.
      The elements below correspond directly to Toshibas CouponInfo structure.
      
      Element Mapping from Toshiba XML:
      - Description: Maps to CouponInfo.Description (coupon description text)
      - DepositFlag: Maps to CouponInfo.DepositFlag (boolean indicating if deposit applies)
      - VoidFlag: Maps to CouponInfo.VoidFlag (boolean indicating coupon is voided)
      - ItemIdentifier: Maps to CouponInfo.ItemIdentifier (item or coupon identifier code)
      - CouponType: Maps to CouponInfo.CouponType (coupon origin, e.g., MANUFACTURER or STORE)
      - Quantity: Maps to CouponInfo.Quantity (number of coupons applied)
      - Value: Maps to CouponInfo.Value (monetary amount discounted, negative values indicate discount)
      - Weight: Maps to CouponInfo.Weight (item weight if applicable, usually 0.00 for coupons)
      - DealPrice: Maps to CouponInfo.DealPrice (promotional deal price, typically 0.00 for coupons)
      - DealQuantity: Maps to CouponInfo.DealQuantity (quantity required for the coupon deal)
      - DepartmentNumber: Maps to CouponInfo.DepartmentNumber (store department number associated with coupon)
      - ItemRepeatAllowedFlag: Maps to CouponInfo.ItemRepeatAllowedFlag (boolean flag indicating if coupon can be repeated)
      - ManufacturerNumber: Maps to CouponInfo.ManufacturerNumber (manufacturer's unique identifier, blank if not provided)
    </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Discount" minOccurs="1" maxOccurs="unbounded">
														<xs:complexType>
															<xs:all>
																<xs:element name="Description" type="xs:string" minOccurs="0"/>
																<xs:element name="DepositFlag" type="xs:boolean" minOccurs="0"/>
																<xs:element name="VoidFlag" type="xs:boolean" minOccurs="0"/>
																<xs:element name="ItemIdentifier" type="xs:string" minOccurs="0"/>
																<xs:element name="CouponType" type="xs:string" minOccurs="0"/>
																<xs:element name="Quantity" type="xs:integer" minOccurs="0"/>
																<xs:element name="Value" type="xs:decimal" minOccurs="0"/>
																<xs:element name="Weight" type="xs:decimal" minOccurs="0"/>
																<xs:element name="DealPrice" type="xs:decimal" minOccurs="0"/>
																<xs:element name="DealQuantity" type="xs:integer" minOccurs="0"/>
																<xs:element name="DepartmentNumber" type="xs:integer" minOccurs="0"/>
																<xs:element name="ItemRepeatAllowedFlag" type="xs:boolean" minOccurs="0"/>
																<xs:element name="ManufacturerNumber" type="xs:string" minOccurs="0"/>
															</xs:all>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Totals" minOccurs="1">
											<xs:complexType>
												<xs:all>
													<xs:element name="SubTotalAmount" type="xs:integer" nillable="true">
														<xs:annotation>
															<xs:documentation>The total price of all items combined, excluding taxes, including order line level discounts, and order level discounts</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DiscountTotal" type="xs:integer" minOccurs="1">
														<xs:annotation>
															<xs:documentation>Total amount of discounts. Used to show how much a customer saved. Postive for sale and negative for return.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DiscountTotalAmount" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The final amount before taxes that a customer is paying</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TotalAmount" type="xs:integer">
														<xs:annotation>
															<xs:documentation>The total amount to be paid including fees and taxes</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TotalShippingAmount" minOccurs="0">
														<xs:annotation>
															<xs:documentation>All shipping data.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:all>
																<xs:element name="ShippingDescription" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Description of this shipping line</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ShippingID" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Unique ID of this shipping line</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ShippingDiscount" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The effective discount of all discounts affecting the shipment, includes overflowing order level discount</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ShippingPrice" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The amount the shipment costs</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ShippingTotal" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The shipment total includes taxes, and discounts</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="TotalHandlingAmount" minOccurs="0">
														<xs:annotation>
															<xs:documentation>All handling data.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:all>
																<xs:element name="HandlingDescription" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Description</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HandlingID" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>ID</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HandlingDiscount" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The effective discount of all discounts affecting the Handling, includes overflowing order level discount</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HandlingPrice" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The amount the Handling costs</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HandlingTotal" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The Handling total includes taxes, and discounts</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="CashBackAmount" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Total amount of money given back to customer.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Balance" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Amount remaining on a payment method</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Change" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Change that the customer was given</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="PaymentOut" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Used for any method that the customer is being paid.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<xs:element name="Description" type="xs:string" minOccurs="0"/>
										<xs:element name="Reason" type="xs:string" minOccurs="0"/>
										<xs:element name="Method" type="xs:string" minOccurs="0"/>
										<xs:element name="Type" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:token">
													<xs:enumeration value="cash"/>
													<xs:enumeration value="amex"/>
													<xs:enumeration value="diners"/>
													<xs:enumeration value="discover"/>
													<xs:enumeration value="jcb"/>
													<xs:enumeration value="maestro"/>
													<xs:enumeration value="maestrouk"/>
													<xs:enumeration value="mastercard"/>
													<xs:enumeration value="uatp"/>
													<xs:enumeration value="visa"/>
													<xs:enumeration value="hipercard"/>
													<xs:enumeration value="aura"/>
													<xs:enumeration value="sorocred"/>
													<xs:enumeration value="cartebleue"/>
													<xs:enumeration value="cartebancaire"/>
													<xs:enumeration value="giftcard"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Amount" type="xs:integer" minOccurs="0"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="TransactionFooter" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="TransactionSurvey" minOccurs="0">
											<xs:complexType>
												<xs:all>
													<xs:element name="Code" type="xs:string"/>
													<xs:element name="Link" type="xs:string"/>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="Line" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Content" type="xs:string"/>
													<xs:element name="ContentStyle" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:all>
																<xs:element name="Style">
																	<xs:simpleType>
																		<xs:restriction base="xs:token">
																			<xs:enumeration value="font"/>
																			<xs:enumeration value="variant"/>
																			<xs:enumeration value="weight"/>
																			<xs:enumeration value="size"/>
																			<xs:enumeration value="color"/>
																			<xs:enumeration value="alignment"/>
																			<xs:enumeration value="decoration"/>
																			<xs:enumeration value="transform"/>
																			<xs:enumeration value="contentindent"/>
																			<xs:enumeration value="letterSpacing"/>
																			<xs:enumeration value="lineHeight"/>
																			<xs:enumeration value="wordSpacing"/>
																			<xs:enumeration value="other"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="StyleValue" type="xs:string"/>
															</xs:all>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="TransactionDocument" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Document" minOccurs="1" maxOccurs="1">
								<xs:complexType>
									<xs:all>
										<xs:element name="DocumentName" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="DocumentType" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="DocumentRefId" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="DocumentMediaType" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="MimeContentType" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="DocuCaptureDateTime" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="DocumentUrl" type="xs:string" minOccurs="0" maxOccurs="1"/>
										<xs:element name="DocumentData" type="xs:string" minOccurs="0" maxOccurs="1"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
</xs:schema>