															<xs:documentation>Simple string that contains Employee Number (used, initially, for employee transactions)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string"/>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Passport" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="PassportID" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Passport number of the recipient</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string"/>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Country" type="CountryTypeCode" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Country of the Passport</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ExpiresDate" type="xs:date" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Date the passport expires</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<!-- Added 2025-09-08 -->
										<xs:element name="LoyaltySummary" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>
      Per-recipient loyalty snapshot tied to this transaction:
      - Earnings: amount earned on this transaction
      - RewardsBalance: current balance after this transaction
      - NextReward: amount remaining until next reward
      - Expiring: date when some balance expires (ISO YYYY-MM-DD)
    </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="ProgramName" type="xs:string" minOccurs="0"/>
													<xs:element name="ProgramId" type="xs:string" minOccurs="0"/>
													<xs:element name="Earnings" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:decimal">
																<xs:fractionDigits value="2"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="RewardsBalance" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:decimal">
																<xs:fractionDigits value="2"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="NextReward" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:decimal">
																<xs:fractionDigits value="2"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Expiring" type="xs:date" minOccurs="0"/>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="LanguageSelection" type="LanguageCode" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The language that the message should be sent in.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="AdditionalMarketingFlag" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Identifies if the recipient is allowed to receive marketing</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:NMTOKEN">
													<xs:enumeration value="YES"/>
													<xs:enumeration value="NO"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Demographics" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Birthday" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Month" nillable="true">
																	<xs:annotation>
																		<xs:documentation>Two digit birth month MM</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:minLength value="2"/>
																			<xs:maxLength value="2"/>
																			<xs:pattern value="[0-9]{2}"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Day" nillable="true">
																	<xs:annotation>
																		<xs:documentation>Two digit birth day</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:minLength value="2"/>
																			<xs:maxLength value="2"/>
																			<xs:pattern value="[0-9]{2}"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Year" nillable="true">
																	<xs:annotation>
																		<xs:documentation>Two digit birth year</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:minLength value="2"/>
																			<xs:maxLength value="2"/>
																			<xs:pattern value="[0-9]{2}"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="Age" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The Age element specifies the age of the customer. This element is optional and must be an integer value between 0 and 120, inclusive.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:integer">
																<xs:minInclusive value="0"/>
																<xs:maxInclusive value="120"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Gender" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Gender of the person if know.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:pattern value="male|female|Unknown|Other"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="TaxExempt" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The TaxExempt element provides information about the customer's tax exempt status. This element is optional and includes the following sub-elements:</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="TaxExemptFlag" minOccurs="0">
														<xs:annotation>
															<xs:documentation>A flag indicating whether the customer has tax exempt status. Valid values are true or false.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:boolean">
																<xs:pattern value="true|false"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="TaxExemptID" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The ID associated with the customer's tax exempt status, if required.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TaxExemptState" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The state or province where the customer's tax exempt status is recognized.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TaxExemptDescription" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Additional description or information regarding the customer's tax exempt status.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ImageFileName" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The ImageFileName element is used to specify the filename of an image file (such as a receipt or invoice) associated with the transaction. This element is optional and should only be used if an image of the receipt or invoice is provided.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="RawTextReceipt" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The RawTextReceipt element is used to contain the raw text representation of the printed receipt. This element is optional and should include the entire receipt text, with each line typically wrapped in "line" tags.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CustomerXML" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The CustomerXML element is used to include non-standard XML elements that are specific to the merchant's system. This element is optional and allows for the inclusion of additional data elements that are not defined in the schema. The content should be in a customer-centric XML format.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="TransactionTextLines" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Simple string that contains text representation of receipt image</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ImageFileText" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element name="PrintLine" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Text to print</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="LF" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Line Feeds</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Bold" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Use Bold</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DH" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Use Double High</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DW" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Use Double Wide</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="LayawayData" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Contains layaway and special order information including payment schedules. This element is optional and applies to transactions involving installment payments or deferred fulfillment. Mapped to /POS/TRADE/ORDER</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="LayawayStatus" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Layaway order status. Mapped to /POS/TRADE/ORDER/Status. Valid values:
- NCMP: Not Completed (Open/In progress)
- SHIP: Shipped (Completed - layaway picked up by customer)
- WOFF: Write Off (Delinquent layaway written off without customer present. Cancel fee collected and credit note created for payments above $10 cancel fee. Next step: waiting for customer to come in to cancel and provide credit)
- CNCL: Cancelled (with customer present)
- PEND: Pending (POS bug - consider completed. Layaway was actually completed but in bad state due to payment bug)
- PSHP: Partially Shipped (POS bug - consider completed. Layaway was actually completed but in bad state due to item status bug)
- COMP: Completed</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayDescription" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>LYWY mapped to /POS/TRADE/ORDER/Desc</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayActionCode" type="xs:integer" minOccurs="0">
								<xs:annotation>
									<xs:documentation>1 - Create, 8 modify/payment, 10 write-off, mapped to /POS/TRADE/ORDER/ActionCode</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayBalanceAdjustAllocatedTax" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>mapped to /POS/TRADE/ORDER/BalanceAdjustAllocatedTax</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayIsPayment" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>If a payment then true, mapped to /POS/TRADE/ORDER/IsPayment</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayBalanceAdjust" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>mapped to /POS/TRADE/ORDER/BalanceAdjust</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayNumber" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Unique order or layaway identifier. Example: 0200301000240, mapped to /POS/TRADE/ORDER/NumStr</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayType" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>LYWY and mapped to /POS/TRADE/ORDER/Type</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayTypeName" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Layaway type name. Common values: Layaway, SPCL (Special Order), HOLD (Hold), etc. mapped to /POS/TRADE/ORDER/TypeName</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayOrigAmount" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Total Layaway amount in cents /POS/TRADE/ORDER/OrigAmt</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayStatusDate" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Date the order or layaway was originally placed /POS/TRADE/ORDER/StatusDate</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayOrigTax" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Total Layaway amount in cents /POS/TRADE/ORDER/OrigTax</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayPlacedDate" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Date the order or layaway was originally placed, mapped to /POS/TRADE/ORDER/PlacedDate</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayBalanceDue" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Remaining balance due in cents, mapped to /POS/TRADE/ORDER/BalanceDue</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LayawayFinalAmount" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Final amount, mapped to /POS/TRADE/ORDER/FinalAmt. Only appears on final payment and pickup.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ScheduledPayments" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Contains a list of scheduled payment installments for the order or layaway.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ScheduledPayment" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Individual scheduled payment installment with payment number, date, and amount. /POS/TRADE/ORDER/ORDER_SCHED_PMTS</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ScheduledPaymentNumber" minOccurs="1">
														<xs:annotation>
															<xs:documentation>Sequential payment number (1, 2, 3, etc.) indicating the installment sequence. mapped to /POS/TRADE/ORDER/ORDER_SCHED_PMTS/SeqNum</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:integer">
																<xs:minInclusive value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="ScheduledPaymentDate" type="xs:dateTime" minOccurs="1">
														<xs:annotation>
															<xs:documentation>Scheduled payment due date in ISO 8601 format (YYYY-MM-DD). Example: 2025-11-19, mapped to /POS/TRADE/ORDER/ORDER_SCHED_PMTS/SchedDate</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ScheduledPaymentAmount" minOccurs="1">
														<xs:annotation>
															<xs:documentation>Scheduled payment amount in cents, mapped to /POS/TRADE/ORDER/ORDER_SCHED_PMTS/Amt</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:decimal">
																<xs:minInclusive value="0"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="TransactionData" minOccurs="1">
					<xs:annotation>
						<xs:documentation>Detailed transaction data.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TransactionSettings" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>The TransactionSettings element contains various settings and parameters related to the transaction. This element is optional and includes the following sub-elements:</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<xs:element name="CurrencyType" type="CurrencyTypeCode" default="USD" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Specifies the currency type for the transaction. The default value is "USD" (U.S. Dollar).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OutputLanguage" type="LanguageCode" default="eng" minOccurs="1">
											<xs:annotation>
												<xs:documentation>Specifies the language code for the transaction output. The default value is "eng" (English).</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TotalAmount" type="xs:integer" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The total amount of the transaction in the specified currency. This is includes all discounts and taxes.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ItemsSold" type="xs:integer" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The total number of items sold in the transaction.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ItemsReturned" type="xs:integer" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The total number of items returned in the transaction.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="BarCode" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Contains information about the barcode associated with the transaction.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="BarCodeFlag" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Flag indicating whether to render the barcode as an image. Valid values are true or false.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:boolean">
																<xs:pattern value="true|false"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="BarCodeType" type="ItemIDTypeCodeEnumeration" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Specifies the type of the barcode.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="BarCodeValue" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The value of the barcode.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<!-- Extended detailed -->
							<xs:element name="DetailedTransactionData" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The DetailedTransactionData element contains detailed information about each line item in the transaction. There should be one instance of this element for each line item. It includes various sub-elements that capture specific attributes and data related to each line item.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<xs:element name="ItemAutoSold" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Indicates if the item is automatically sold with a parent item, such as a Tire Levy, Tire Fee, or Enviro Fee. If the item is auto-sold, the value should be "true".</xs:documentation>
												<xs:appinfo source="FlooidPOS">This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetail/autosold.</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="ProductIDType" type="ItemIDTypeCodeEnumeration" minOccurs="1">
											<xs:annotation>
												<xs:documentation>Specifies the type of product identifier being used. This is a required element and should use values from the ItemIDTypeCodeEnumeration enumeration.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ProductId" type="xs:string" minOccurs="1">
											<xs:annotation>
												<xs:documentation>The unique identifier for the product. This is a required element and should be a string value.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Description" type="xs:string" minOccurs="1">
											<xs:annotation>
												<xs:documentation>The name or description of the product. This is a required element and should provide a clear, descriptive name for the product.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<!-- TT: 04-08-2022 Ticket#SR-11202493 -->
										<xs:element name="OriginalTransId" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element should be included if the item is being returned. It contains the transaction ID from the original or parent transaction.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OriginalOrderLineSeq" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element should be included if the item is being returned. It contains the line sequence number of the item from the original or parent transaction.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OriginalStoreNumber" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element should be included if the item is being returned. It contains the Store Number from the original or parent transaction. Added on 9/27/2024</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OriginalRegisterNumber" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element should be included if the item is being returned. It contains the Register Number from the original or parent transaction. Added on 9/27/2024</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OriginalTransactionDate" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element should be included if the item is being returned. It contains the transaction date from the original or parent transaction. Added on 9/27/2024.  Format is YYYY-MM-DD.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="OriginalStoreTaxArea" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This element should be included if the item is being returned. It contains the store tax area from the original or parent transaction. Added on 9/27/2024</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DepartmentCode" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The code representing the department to which the item belongs. This element is optional and should be a string value.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DepartmentName" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The name of the department to which the item belongs. This element is optional and should be a string value.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="GiftReceipt" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>For a sale, this element identifies if the item was a gift. Valid values are TRUE or FALSE.</xs:documentation>
												<xs:appinfo source="FlooidPOS">This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetail/giftReceipt.</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="ItemLevel" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The level of the product within a hierarchy of linked products. The default value is 0.</xs:documentation>
												<xs:appinfo source="FlooidPOS">This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetail/level.</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<!-- TT: 2022-09-06 mapped to TxnDetail/itemVoid -->
										<xs:element name="ItemVoid" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Set to 1 if the line item was voided.</xs:documentation>
												<xs:appinfo source="FlooidPOS">This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetail/itemVoid.</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="ItemVoidLine" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The parent number of this voiding Line</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Quantity" minOccurs="1">
											<xs:annotation>
												<xs:documentation>The Quantity element provides details about the quantity of items involved in the transaction. This element is required and includes the following sub-elements:</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="QuantitySoldType" type="ItemUnits" minOccurs="1">
														<xs:annotation>
															<xs:documentation>Specifies the type of units for the quantity sold, such as pieces, kilograms, liters, etc. This is a required element.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="QuantitySold" type="xs:integer" minOccurs="1">
														<xs:annotation>
															<xs:documentation>The number of units sold, represented as an integer by multiplying the actual quantity by 100.

IMPORTANT - Integer Representation:
- Actual quantity 1.00 = 100 (integer)
- Actual quantity 1.50 = 150 (integer)
- Actual quantity 100.00 = 10000 (integer)
- Actual quantity 0.25 = 25 (integer)

This format allows for precise representation of fractional quantities (up to 2 decimal places) while using integer storage.

This is a required element. The value should be positive if it represents a purchase and negative if it represents a return.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="ItemStatus" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The ItemStatus element provides details about the status of the item within the transaction. This element is optional and includes the following sub-elements:</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:all>
													<xs:element name="ItemReceivedType" minOccurs="0">
														<xs:simpleType>
															<xs:union memberTypes="xs:string xs:integer">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:enumeration value="Delivered">
																			<xs:annotation>
																				<xs:documentation>The item has been delivered.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Pickedup">
																			<xs:annotation>
																				<xs:documentation>The item has been picked up</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="WarehousePickup">
																			<xs:annotation>
																				<xs:documentation>The item has been picked up from the warehouse</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Store">
																			<xs:annotation>
																				<xs:documentation>The item is available in the store.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="DropShipped">
																			<xs:annotation>
																				<xs:documentation>The item has been drop-shipped.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Shipped">
																			<xs:annotation>
																				<xs:documentation>The item has been shipped.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="OTHER">
																			<xs:annotation>
																				<xs:documentation>The item has a different status not specified in the list.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="UNKNOWN">
																			<xs:annotation>
																				<xs:documentation>The status of the item is unknown.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Ordered">
																			<xs:annotation>
																				<xs:documentation>The item has been ordered.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Ready">
																			<xs:annotation>
																				<xs:documentation>The item is ready for pickup or delivery.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Return">
																			<xs:annotation>
																				<xs:documentation>The item is being returned.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="TradeIn">
																			<xs:annotation>
																				<xs:documentation>The item is a trade-in.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="EcoFee">
																			<xs:annotation>
																				<xs:documentation>The item includes an ecological fee.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Exchange">
																			<xs:annotation>
																				<xs:documentation>The item is being exchanged.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																		<xs:enumeration value="Void">
																			<xs:annotation>
																				<xs:documentation>The item is voided.</xs:documentation>
																			</xs:annotation>
																		</xs:enumeration>
																	</xs:restriction>
																</xs:simpleType>
																<xs:simpleType>
																	<xs:restriction base="xs:integer">
																		<xs:minInclusive value="1"/>
																		<xs:maxInclusive value="100"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:union>
														</xs:simpleType>
													</xs:element>
													<xs:element name="ItemStatusDate" type="xs:dateTime" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The date and time when the item status was updated. This element is optional.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ItemReceivedDate" type="xs:dateTime" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The date and time when the item was received. This element is optional.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="Price" minOccurs="1">
											<xs:complexType>
												<xs:all>
													<xs:element name="DollarAmount" type="xs:integer" minOccurs="1">
														<xs:annotation>
															<xs:documentation>The unit price of an item before any discounts. This value should always be positive.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DiscountDollarAmount" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>
                                                                UPDATE: Postive value for sale and negative value for return. This is per a unit.
                                                                
                                                                The discount applied to the line per unit. Total discount of all units purchased. This element is optional. The value should be a negative integer if it represents a purchase and a positive integer if it represents a return.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ExtDollarAmount" type="xs:integer" minOccurs="1">
														<xs:annotation>
															<xs:documentation>The final unit price after discount before taxes multiplied by the number of units</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="OrginalAmount" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The unit price after discount but before taxes. Should always be a positive value. This element is optional and should be an integer value.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ReturnAmount" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The value per unit that can be returned. This element is optional and should be a positive integer value. If the item is being returned, the value should be 0. This value represents the final price a customer is paying per unit before taxes.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ItemCost" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Cost the merchant pays per an item.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="OverridePrice" type="xs:integer" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Override Price added 10/16/2024</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="PriceRule" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Rule used to determine the price. added 10/16/2024</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
										<xs:element name="ItemDiscounts" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Discount information at the line item level.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Discount" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:all>
																<xs:element name="DiscountPercent" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The percentage of the discount applied to the item.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountAmount" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>This value should be negative for sale and positive for return. This is the unit discount x discount value. The final dollar amount of the discount.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="AmountDiscountable" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The amount of the item that this discount applies to.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetailDiscount/amountDiscountable.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountPromoCode" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The promotion code used for the discount.</xs:documentation>
																		<xs:appinfo source="FlooidPOS">This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetailDiscount/promotionCode.</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountVoided" type="xs:boolean" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Indicates if the discount was voided. Valid values are true or false.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetailDiscount/voided.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountVoidedLine" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The parent number of the voiding line.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid TxnDetailDiscount/voidingLine.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountDescription" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>A description of the discount.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountReasonCode" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The reason code for the discount.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountReasonCodeType" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The type of the reason code for the discount.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountingMethod" minOccurs="0">
																	<xs:simpleType>
																		<xs:restriction base="xs:NMTOKEN">
																			<xs:enumeration value="DollarOff">
																				<xs:annotation>
																					<xs:documentation>The discount is a specific dollar amount off.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="PercentOff">
																				<xs:annotation>
																					<xs:documentation>The discount is a percentage off.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="NewPrice">
																				<xs:annotation>
																					<xs:documentation>The discount is a new price for the item.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="1"/>
																			<xs:enumeration value="2"/>
																			<xs:enumeration value="3"/>
																			<xs:enumeration value="4"/>
																			<xs:enumeration value="5"/>
																			<xs:enumeration value="6"/>
																			<xs:enumeration value="7"/>
																			<xs:enumeration value="8"/>
																			<xs:enumeration value="9"/>
																			<xs:enumeration value="10"/>
																			<!--JS ADD 2022-06-24-->
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="DiscountingRate" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Rate of the discount. Value depends on DiscountingMethod. If the method is DollarOff/NewPrice then this will be the dollar amount, if PercentOff the the percent value (eg: 10% would be stored as 10)</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="PrintLine" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Formatted print line for this discount/promotion</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="Reason" minOccurs="0" maxOccurs="1">
																	<xs:complexType>
																		<xs:all>
																			<!--JS Modify Code from integer to string 9/5/2024-->
																			<xs:element name="Code" type="xs:string" minOccurs="1" maxOccurs="1">
																				<xs:annotation>
																					<xs:documentation>The code for the reason of the discount.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="1">
																				<xs:annotation>
																					<xs:documentation>The description for the reason of the discount.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="PrintLine" type="xs:string" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Formatted print line for this reason</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:all>
																	</xs:complexType>
																</xs:element>
																<xs:element name="PromotionData" minOccurs="0" maxOccurs="1">
																	<xs:complexType>
																		<xs:all>
																			<xs:element name="Identifier" type="xs:string" minOccurs="1" maxOccurs="1"/>
																			<xs:element name="EventNumber" type="xs:integer" minOccurs="1" maxOccurs="1"/>
																			<xs:element name="Method" minOccurs="1" maxOccurs="1">
																				<xs:simpleType>
																					<xs:restriction base="xs:NMTOKEN">
																						<xs:enumeration value="BuyToGet"/>
																						<xs:enumeration value="BuyOneGetOne"/>
																						<xs:enumeration value="BuyOneGetAnother"/>
																						<xs:enumeration value="BuyToGetPlus"/>
																						<xs:enumeration value="MinimunPurchaseTriggerOnly"/>
																						<xs:enumeration value="MinimunPurchaseTriggerNontrigger"/>
																						<xs:enumeration value="MinimunPurchaseAll"/>
																						<xs:enumeration value="Bundled"/>
																						<xs:enumeration value="1"/>
																						<xs:enumeration value="2"/>
																						<xs:enumeration value="3"/>
																						<xs:enumeration value="4"/>
																						<xs:enumeration value="5"/>
																						<xs:enumeration value="6"/>
																						<xs:enumeration value="7"/>
																						<xs:enumeration value="8"/>
																						<xs:enumeration value="9"/>
																						<xs:enumeration value="10"/>
																						<!--JS ADD 2022-06-24-->
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element name="TriggerAmount" type="xs:integer" minOccurs="1" maxOccurs="1">
																				<xs:annotation>
																					<xs:documentation>This is the amount required to meet the Promotion. Will be a dollar value for MinimunPurchase Methods, otherwise it will be quantity-based.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:all>
																	</xs:complexType>
																</xs:element>
																<xs:element name="EnhancedCoupon" minOccurs="0" maxOccurs="1">
																	<xs:annotation>
																		<xs:documentation>A coupon with discounting parameters pre-defined in the database. Can also be a requirement for a Promotion to applied.</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:all>
																			<xs:element name="Identifier" type="xs:string" minOccurs="1" maxOccurs="1"/>
																			<xs:element name="EntryMethod" minOccurs="1" maxOccurs="1">
																				<xs:simpleType>
																					<xs:restriction base="xs:NMTOKEN">
																						<xs:enumeration value="FromMenu"/>
																						<xs:enumeration value="Scanned"/>
																						<xs:enumeration value="HandKeyed"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element name="Barcode" type="xs:string" minOccurs="0" maxOccurs="1"/>
																			<xs:element name="TransactionLogReasonCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
																			<xs:element name="ReasonCodeDescription" type="xs:string" minOccurs="1" maxOccurs="1"/>
																			<xs:element name="IsDealTrigger" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
																		</xs:all>
																	</xs:complexType>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
													<xs:element name="GroupDiscount" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Itemized data for group-/transaction-level discounts.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:all>
																<xs:element name="Identifier" type="xs:integer" minOccurs="1" maxOccurs="1">
																	<xs:annotation>
																		<xs:documentation>Identifier that groups the items together. This combined with DiscountType will uniquely identify a group/transaction discount.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="DiscountType" minOccurs="1" maxOccurs="1">
																	<xs:annotation>
																		<xs:documentation>Specific type of group/trasnaction discount. An item cannot have multiple discounts of the same type. This combined with Identifier will uniquely identify a group/transaction discount.</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:NMTOKEN">
																			<xs:enumeration value="ManualTransaction"/>
																			<xs:enumeration value="ManualGroup"/>
																			<xs:enumeration value="EmployeeTransaction"/>
																			<xs:enumeration value="EmployeeGroup"/>
																			<xs:enumeration value="PreferredCustomer"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="DiscountLevel" minOccurs="1" maxOccurs="1">
																	<xs:simpleType>
																		<xs:restriction base="xs:NMTOKEN">
																			<xs:enumeration value="Group"/>
																			<xs:enumeration value="Transaction"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="DiscountAmount" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The final dollar amount of the discount.</xs:documentation>
																	</xs:annotation>
																</xs:element>
