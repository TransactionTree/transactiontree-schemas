														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ItemTax" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Contains tax information at the line item level. This can include various types of taxes such as state, provincial, GST, HST, special taxes, and others.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Tax" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:all>
																<xs:element name="TaxType" minOccurs="0">
																	<xs:simpleType>
																		<xs:restriction base="xs:NMTOKEN">
																			<xs:enumeration value="ECO">
																				<xs:annotation>
																					<xs:documentation>Eco Fee, added 9/27/2024.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="GST">
																				<xs:annotation>
																					<xs:documentation>Goods and Services Tax.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="HST">
																				<xs:annotation>
																					<xs:documentation>Harmonized Sales Tax.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="ItemTax">
																				<xs:annotation>
																					<xs:documentation>General tax applied to the item.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="OtherTax">
