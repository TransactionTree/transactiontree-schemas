													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ItemAttributes" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Attribute" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:all>
																<xs:element name="AttributeType">
