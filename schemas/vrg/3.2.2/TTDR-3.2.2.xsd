<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2026 (x64) (https://www.altova.com) by Jason Shapiro (Private) -->
<xs:schema xmlns="https://www.transactiontree.com/framework/schema/vrg/3.2.2/TTDR3.2.2.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="https://www.transactiontree.com/framework/schema/vrg/3.2.2/TTDR3.2.2.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" version="3.2.2" xml:lang="en">
	<xs:include schemaLocation="TTDRsimpleTypes-3.2.2.xsd"/>
	<!-- TransactionTree Digital Receipt version Current Version 3.2.2 03/16/2021 UPDATED 2025-09-08 -->
	<!-- Copyright 2025, TransactionTree, Inc. -->
	<xs:element name="Transaction">
		<xs:complexType>
			<xs:all>
				<xs:element name="TTDRVersion" type="xs:string" fixed="TTDRV322" nillable="false" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The TTDRVersion element specifies the version of the TTDR schema being used. The current value is fixed to "TTDRV322" to indicate this specific version.</xs:documentation>
						<xs:appinfo>This element is used to ensure compatibility and version control in the TransactionTree system.</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicationID" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The ApplicationID element identifies the application that is sending this message. It is an optional field and can be used to track the source application.</xs:documentation>
						<xs:appinfo>This element helps in identifying and managing messages from different applications within the TransactionTree system.</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="TransactionHistory" type="xs:string" default="N" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Historical transaction indicatore (Y,N). Y when transaction was not generated in real time.  Added 9/27/2024</xs:documentation>
						<xs:appinfo>This element helps in identifying historical transaction being processed by the TransactionTree system.</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="MerchantNumber" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The MerchantNumber element represents a unique identifier assigned by TransactionTree. It must be a numeric value consisting of 6 to 9 digits.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="9"/>
							<xs:pattern value="[0-9]*"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="TransactionDateTime" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The transaction date and time</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="1" maxOccurs="1">
							<xs:element name="TransactionDate" type="xs:date" minOccurs="1">
								<xs:annotation>
									<xs:documentation>The TransactionDate element represents a date value in the format YYYY-MM-DD. This element is used to specify the date of the transaction in the TransactionTree system.</xs:documentation>
									<xs:appinfo>This element is used to record the transaction date in the system.</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="TransactionTime" type="xs:time" minOccurs="1">
								<xs:annotation>
									<xs:documentation>The TransactionTime element represents a time value in the format hh:mm:ss. It can also include optional fractional seconds and an optional time zone offset.</xs:documentation>
									<xs:appinfo>This element is used to specify the transaction time in the TransactionTree system.</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<!-- Optional Elements -->
							<xs:element name="TransactionStart" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The date and time when the transaction began. Added 6/17/2025.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TransactionEnd" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The date and time when the transaction was completed. Added 6/17/2025.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ReceiptDateTime" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The date and time printed on the receipt. Added 6/17/2025.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="EntryType" default="PRINT" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The EntryType element specifies the method by which the message should be handled and delivered. This includes options such as printing, emailing, sending via SMS, pushing to an application, or combinations thereof. The default value is "PRINT".</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="EMAIL">
								<xs:annotation>
									<xs:documentation>Message is only to be emailed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PRINT">
								<xs:annotation>
									<xs:documentation>Message is only to be printed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="BOTH">
								<xs:annotation>
									<xs:documentation>Message is only to be emailed and printed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="NOPRINT">
								<xs:annotation>
									<xs:documentation>Message is not to be printed or sent</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="NOPRINT_EMAIL">
								<xs:annotation>
									<xs:documentation>Message is only to be emailed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="SMS">
								<xs:annotation>
									<xs:documentation>Message is only to be sent via SMS</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="NOPRINT_SMS">
								<xs:annotation>
									<xs:documentation>Message is only to be sent via SMS</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PRINT_SMS">
								<xs:annotation>
									<xs:documentation>Message is to be sent via SMS and printed.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="EMAIL_SMS">
								<xs:annotation>
									<xs:documentation>Message is to be sent via SMS and Email, but not printed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="EMAIL_SMS_PRINT">
								<xs:annotation>
									<xs:documentation>Message is to be sent via SMS, Email, and printed.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PUSH">
								<xs:annotation>
									<xs:documentation>Message is only to be pushed to an application</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PUSH_PRINT">
								<xs:annotation>
									<xs:documentation>Message is to be pushed to application and printed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PUSH_EMAIL">
								<xs:annotation>
									<xs:documentation>Message is to be pushed to application and emailed, but not printed</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PUSH_BOTH">
								<xs:annotation>
									<xs:documentation>Message is to be pushed to application, printed, and emailed.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="ALL">
								<xs:annotation>
									<xs:documentation>Message is to be pushed to application, printed, emailed, and sent via sms.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="SMS_LINK">
								<xs:annotation>
									<xs:documentation>Send message to complete process.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="NONE">
								<xs:annotation>
									<xs:documentation>Do not send this message out only store the data.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="SMS_REGISTER">
								<xs:annotation>
									<xs:documentation>Send sms message to complete registration</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="SMS_UPDATE">
								<xs:annotation>
									<xs:documentation>Send sms message to update profile</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="TransId" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The TransId element is a string used to identify a unique transaction number or rental record associated with a transaction. It is optional and can be used to uniquely track and reference individual transactions within the system. The identifier must be alphanumeric and between 1 and 50 characters in length.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="OriginalTransId" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The OriginalTransId element is used to tie this transaction to a parent or prior transaction. All DetailedTransactionData below will be tied to this value. Use this element only if all items in this transaction are to be connected to the parent or prior transaction. The identifier must be a unique alphanumeric string and between 1 and 50 characters in length.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FormId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Simple string to identify form# associated with transaction</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="TransactionType" nillable="false" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The TransactionType element specifies the type of transaction being processed. This element is required and must have one of the following enumerated values.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="SaleTransaction">
								<xs:annotation>
									<xs:documentation>Normal sale</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="ReturnTransaction">
								<xs:annotation>
									<xs:documentation>Normal return</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="ExchangeTransaction">
								<xs:annotation>
									<xs:documentation>Normal Exchange</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="EmployeeSale">
								<xs:annotation>
									<xs:documentation>Sale transaction where the person making the purchase is an employee</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="EmployeeExchange">
								<xs:annotation>
									<xs:documentation>Exchange transaction where the person making the exchange is an employee</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="EmployeeReturn">
								<xs:annotation>
									<xs:documentation>Return transaction where the person making the return is an employee</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="TaxExemptSale">
								<xs:annotation>
									<xs:documentation>Sale transaction that is tax-exempt</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="TaxExemptExchange">
								<xs:annotation>
									<xs:documentation>Exchange transaction that is tax-exempt</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="TaxExemptReturn">
								<xs:annotation>
									<xs:documentation>Return transaction that is tax-exempt</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Reprint">
								<xs:annotation>
									<xs:documentation>Reprint of a previous transaction receipt</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="REGISTER">
								<xs:annotation>
									<xs:documentation>Registration transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="WEBORDER">
								<xs:annotation>
									<xs:documentation>Transaction originating from a web order</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="MOBILE">
								<xs:annotation>
									<xs:documentation>Transaction originating from a mobile device</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PlatinumPrePrint">
								<xs:annotation>
									<xs:documentation>Pre-printing of a Platinum membership transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PlatinumComplete">
								<xs:annotation>
									<xs:documentation>Completion of a Platinum membership transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="GoldChoiceRent">
								<xs:annotation>
									<xs:documentation>Gold Choice rental transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CustomerSatRecord">
								<xs:annotation>
									<xs:documentation>Customer satisfaction record transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="InstantReturn">
								<xs:annotation>
									<xs:documentation>Instant return transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="ManualRA">
								<xs:annotation>
									<xs:documentation>Manual rental agreement</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PostRent">
								<xs:annotation>
									<xs:documentation>Post-rent transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PostReturn">
								<xs:annotation>
									<xs:documentation>Post-return transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Return">
								<xs:annotation>
									<xs:documentation>Return transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="MobileReturn">
								<xs:annotation>
									<xs:documentation>Return transaction originating from a mobile device</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Rent">
								<xs:annotation>
									<xs:documentation>Rental transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="UpdateOptServices">
								<xs:annotation>
									<xs:documentation>Update of optional services</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="VehicleExchange">
								<xs:annotation>
									<xs:documentation>Vehicle exchange transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="GoldChoiceExit">
								<xs:annotation>
									<xs:documentation>Gold Choice exit transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="GoldService">
								<xs:annotation>
									<xs:documentation>Gold service transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="AddDriver">
								<xs:annotation>
									<xs:documentation>Addition of a driver to a rental agreement</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CreditCheck">
								<xs:annotation>
									<xs:documentation>Credit check transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="LayAway">
								<xs:annotation>
									<xs:documentation>Layaway transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Voided">
								<xs:annotation>
									<xs:documentation>Voided transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="RemotePay">
								<xs:annotation>
									<xs:documentation>Remote payment transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="NonTransaction">
								<xs:annotation>
									<xs:documentation>Non-transactional activity</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="OrderNotification">
								<xs:annotation>
									<xs:documentation>Order notification transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="unknown">
								<xs:annotation>
									<xs:documentation>Unknown transaction type</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Delivery">
								<xs:annotation>
									<xs:documentation>Delivery transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CurbsidePickup">
								<xs:annotation>
									<xs:documentation>Curbside pickup transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="InStorePickup">
								<xs:annotation>
									<xs:documentation>In-store pickup transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Update">
								<xs:annotation>
									<xs:documentation>Update transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Sale">
								<xs:annotation>
									<xs:documentation>Sale transaction</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PostVoidSale">
								<xs:annotation>
									<xs:documentation>A sale that has been voided in full on the same day of the orginal transaction. Added on 11/11/2025.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="PostVoidReturn">
								<xs:annotation>
									<xs:documentation>A return that has been voided in full on the same day of the orginal transaction. Added on 11/11/2025.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="TransactionQualifier" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The TransactionQualifier element provides additional qualifiers for a transaction, indicating special conditions or statuses associated with it. These element is optional and may contain the following sub-elements.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="IsEmployeeTransaction" type="xs:boolean" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Indicates whether the transaction is an employee purchase. A value of true means it is an employee transaction; false or omitted means it is not.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="IsSendTransaction" type="xs:boolean" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Indicates whether the transaction involves sending the items (e.g., shipping). A value of true means it is a send transaction; false or omitted means it is not.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="IsTaxExemptTransaction" type="xs:boolean" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Indicates whether the transaction is tax-exempt. A value of true means it is a tax-exempt transaction; false or omitted means it is not.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TaxExemptCertificate" type="xs:string" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>The tax exempt certificate ID associated with the transaction. This element is optional and should be provided if the transaction is tax-exempt and a certificate ID is available.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="TransactionLocation" minOccurs="1">
					<xs:annotation>
						<xs:documentation>The TransactionLocation element provides details about the location where the transaction occurred. This element is required and includes the following sub-elements.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<!-- 1 to 9 digit numeric store number determined by retailer to identify origin store -->
							<xs:element name="StoreNumber" minOccurs="1">
								<xs:annotation>
									<xs:documentation>Unique location identifier from the sending application where the data is being generated. This identifier must be between 1 and 9 alphanumeric characters.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="9"/>
										<xs:pattern value="[A-Z0-9]*"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="LocationType" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:NMTOKEN">
										<xs:enumeration value="purchasepickupLocation">
											<xs:annotation>
												<xs:documentation>Location where the purchase took place and items are picked up from.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="purchaseLocation">
											<xs:annotation>
												<xs:documentation>Location where the purchase took place only</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="pickupLocation">
											<xs:annotation>
												<xs:documentation>Location where the items are to be picked up only</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="otherLocation">
											<xs:annotation>
												<xs:documentation>Non-standard location.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="returnLocation">
											<xs:annotation>
												<xs:documentation>Location that return took place.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RegisterNumber">
								<xs:annotation>
									<xs:documentation>1 to 9 digit numeric register number determined by retailer to identify origin register</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="9"/>
										<xs:pattern value="[A-Z0-9]*"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RegisterType" minOccurs="1" default="POS">
								<xs:annotation>
									<xs:documentation>The RegisterType element represents different types of registers that can be used in a retail or transaction-based system. Predefined values are provided for common register types, but custom values are also allowed for extensibility.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:union>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="POS">
													<xs:annotation>
														<xs:documentation>Point of Sale (POS) register.</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="SmartPOS">
													<xs:annotation>
														<xs:documentation>Smart POS with cloud or AI capabilities.</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="CallCenterStation">
													<xs:annotation>
														<xs:documentation>Call center station.</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="Network">
													<xs:annotation>
