																					<xs:documentation>Other types of taxes not specified.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="PST">
																				<xs:annotation>
																					<xs:documentation>Provincial Sales Tax, added 9/27/2024.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="QST">
																				<xs:annotation>
																					<xs:documentation>Quebec Sales Tax, added 9/27/2024.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="RST">
																				<xs:annotation>
																					<xs:documentation>Retail Sales Tax, added 9/27/2024.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="SpecialTax">
																				<xs:annotation>
																					<xs:documentation>Any special tax applicable to the item.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="StateOrProvince">
																				<xs:annotation>
																					<xs:documentation>Tax imposed by the state or province.</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="TaxAmount" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The amount of tax applied to the item. This should be an integer value representing the tax amount in the smallest currency unit (e.g., cents).</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxFlag" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>A flag indicating special conditions or statuses for the tax. "true" if tax applies or "false" if not.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxPercent" type="xs:decimal" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The percentage rate of the tax applied to the item. This should be a decimal value.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxDescription" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>A description of the tax applied to the item.</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxAuthority" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The authority that imposes the tax. This could be the name or code of the tax authority.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid txnDetailTaxes/taxAuthorityCode.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxRuleCode" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The code of the tax rule applied to the item. This code identifies the specific rule under which the tax is applied.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid txnDetailTaxes/taxRuleCode.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxableAmount" type="xs:integer" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The amount of the item that is subject to tax. This should be an integer value representing the amount in the smallest currency unit (e.g., cents).</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid txnDetailTaxes/valueTaxable.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxVoided" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Indicates if the tax entry has been voided. Valid values are "true" or "false". When "true" tax line has been voided.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid txnDetailTaxes/taxVoided.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxVoidedLine" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The parent number of the voiding line for the tax entry.</xs:documentation>
																		<xs:appinfo>
																			<application name="FlooidPOS">
																				<xs:documentation>This element was added on 2022-09-06 specifically for the Flooid POS system and is mapped to Flooid txnDetailTaxes/voidingLine.</xs:documentation>
																			</application>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="TaxStartDate" type="xs:string" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>The TaxStartDate element represents a date value in the format YYYY-MM-DD. This element is used to specify the date of the when this tax rate was started in the TransactionTree system. Added 9/27/2024</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ItemAddOn" minOccurs="0">
